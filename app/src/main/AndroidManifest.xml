<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.loubinfeng.www.bezierdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Bezier2Activity"
            android:label="@string/title_activity_bezier2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Bezier3Activity"
            android:label="@string/title_activity_bezier3"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DrawPadActivity"
            android:label="@string/title_activity_draw_pad"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PathMorphActivity"
            android:label="@string/title_activity_path_morph"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>